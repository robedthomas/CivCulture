<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!-- Fundamentals -->
    <Image x:Key="FoodIcon" Source="FoodIcon.png"/>
    <Image x:Key="MoneyIcon" Source="MoneyIcon.png"/>
    <Image x:Key="ProductionIcon" Source="ProductionIcon.png"/>
    <Image x:Key="ShelterIcon" Source="ShelterIcon.png"/>
    
    <!-- Resources -->
    
    <!-- Terrain Resources -->
    <Image x:Key="WheatIcon" Source="WheatIcon.png"/>
    <Image x:Key="WildernessIcon" Source="WildernessIcon.png"/>

    <!-- Buildings -->
    <Image x:Key="MudHutsIcon" Source="MudHutsIcon.png"/>
    <Image x:Key="PrimitiveFarm_GrasslandIcon" Source="PrimitiveFarmIcon.png"/>
    <Image x:Key="PrimitiveFarm_WheatIcon" Source="PrimitiveFarmIcon.png"/>
    
    <!-- Building Backgrounds -->
    <SolidColorBrush x:Key="MudHutsBackground" Color="Brown" />
    <SolidColorBrush x:Key="PrimitiveFarm_GrasslandBackground" Color="Green" />
    <SolidColorBrush x:Key="PrimitiveFarm_WheatBackground" Color="DarkGoldenrod" />
</ResourceDictionary>